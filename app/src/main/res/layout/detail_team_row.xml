<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bodyLayout">


    <ImageView
        android:id="@+id/iv_logotim_detailteam"
        android:layout_width="126dp"
        android:layout_height="126dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="32dp"
        android:background="@drawable/placeholder_logotim"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_namatim_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginLeft="32dp"
        app:layout_constraintTop_toBottomOf="@+id/iv_logotim_detailteam">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:paddingTop="11dp"
            android:fontFamily="@font/poppins_semibold"
            android:text="Nama Tim:"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/et_namatim_detailteam"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:layout_marginLeft="16dp"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="10dp"
            android:textAlignment="center"
            android:background="@drawable/klik"
            app:layout_constraintStart_toEndOf="@+id/textView10" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_kategoritim_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="32dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_namatim_detailteam">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:paddingTop="11dp"
            android:fontFamily="@font/poppins_semibold"
            android:text="Season:"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/et_seasontim_detailteam"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:layout_marginLeft="16dp"
            android:paddingRight="12dp"
            android:inputType="date"
            android:fontFamily="@font/poppins_semibold"
            android:textAlignment="center"
            android:background="@drawable/klik"
            android:drawableEnd="@drawable/ic_calendar"
            app:layout_constraintStart_toEndOf="@+id/textView11" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_coach_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_kategoritim_detailteam">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="Nama Coach:"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/et_coachtim_detailteam"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:layout_marginLeft="16dp"
            android:background="@drawable/klik"
            android:fontFamily="@font/poppins_semibold"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/textView12" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_asisten_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_coach_detailteam">

        <TextView
            android:id="@+id/textView13"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="Nama Asisten:"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/et_asistentim_detailteam"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:layout_marginLeft="16dp"
            android:background="@drawable/klik"
            android:fontFamily="@font/poppins_semibold"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/textView13" />

    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.constraintlayout.widget.ConstraintLayout>