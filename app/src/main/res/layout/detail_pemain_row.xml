<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/bodyLayout">


    <ImageView
        android:id="@+id/iv_profilpemain_detailpemain"
        android:layout_width="126dp"
        android:layout_height="126dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="32dp"
        android:background="@drawable/placeholder_logotim"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_namatim_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginLeft="32dp"
        app:layout_constraintTop_toBottomOf="@+id/iv_logotim_detailteam">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="Nama Pemain:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_namapemain_detailpemain"
            android:hint="Nama Lengkap"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:layout_marginLeft="16dp"
            android:background="@drawable/klik"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="10dp"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/textView10"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_kategoritim_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="32dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_namatim_detailteam">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="Posisi:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_posisi_pemain_detailpemain"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:layout_marginLeft="16dp"
            android:paddingRight="16dp"
            android:drawableEnd="@drawable/ic_dropdown"
            android:hint="Pilih ->"
            android:background="@drawable/klik"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="10dp"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/textView11"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_statuspemain_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="32dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_namatim_detailteam">

        <TextView
            android:id="@+id/tv11"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="Status:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_statuspemain_detailpemain"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:layout_marginLeft="16dp"
            android:drawableEnd="@drawable/ic_dropdown"
            android:hint="Pilih ->"
            android:paddingRight="16dp"
            android:background="@drawable/klik"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="10dp"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/tv11"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_coach_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_kategoritim_detailteam">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="No. Punggung:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_nomer_punggung_detailpemain"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:hint="contoh: 00"
            android:inputType="number"
            android:layout_marginLeft="16dp"
            android:background="@drawable/klik"
            android:fontFamily="@font/poppins_semibold"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/textView12"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_asisten_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_coach_detailteam">

        <TextView
            android:id="@+id/textView13"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="Lateralitas:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_lateralitas_pemain_detailpemain"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:hint="Pilih ->"
            android:drawableEnd="@drawable/ic_dropdown"
            android:layout_marginLeft="16dp"
            android:background="@drawable/klik"
            android:paddingRight="16dp"
            android:fontFamily="@font/poppins_semibold"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/textView13"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_jersey_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_asisten_detailteam">

        <TextView
            android:id="@+id/textView14"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="Berat Badan:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_beratbadan_pemain_detailpemain"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:layout_marginLeft="16dp"
            android:hint="contoh: 66.7(kg)"
            android:background="@drawable/klik"
            android:inputType="numberDecimal"
            android:fontFamily="@font/poppins_semibold"
            android:paddingRight="12dp"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/textView14"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_instansi_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_jersey_detailteam">

        <TextView
            android:id="@+id/textView15"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:inputType="numberDecimal"
            android:text="BMI:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_bmi_pemain_detailpemain"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:hint="contoh: 22.7"
            android:layout_marginLeft="16dp"
            android:background="@drawable/klik"
            android:fontFamily="@font/poppins_semibold"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/textView15"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_alamat_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_instansi_detailteam">

        <TextView
            android:id="@+id/textView16"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="Tanggal Lahir:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_tanggal_lahir_detailpemain"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:hint="klik icon ->"
            android:layout_marginLeft="16dp"
            android:background="@drawable/klik"
            android:drawableEnd="@drawable/ic_calendar"
            android:fontFamily="@font/poppins_semibold"
            android:inputType="date"
            android:paddingRight="12dp"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/textView16"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_kotakab_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_alamat_detailteam">

        <TextView
            android:id="@+id/textView17"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="Jenis Kelamin:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_jeniskelamin_pemain_detailpemain"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:hint="Pilih ->"
            android:layout_marginLeft="16dp"
            android:background="@drawable/klik"
            android:fontFamily="@font/poppins_semibold"
            android:textAlignment="center"
            android:paddingRight="12dp"
            android:drawableEnd="@drawable/ic_dropdown"
            app:layout_constraintStart_toEndOf="@+id/textView17"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_provinsi_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_kotakab_detailteam">

        <TextView
            android:id="@+id/textView18"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="Domisili:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_domisili_pemain_detailpemain"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:hint="contoh: malang"
            android:layout_marginLeft="16dp"
            android:background="@drawable/klik"
            android:fontFamily="@font/poppins_semibold"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/textView18"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!--    negara-->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_negara_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_provinsi_detailteam">

        <TextView
            android:id="@+id/textView19"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="Nomor Hp:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/et_nomorhp_pemain_detailpemain"
            android:layout_width="189dp"
            android:layout_height="40dp"
            android:inputType="number"
            android:hint="contoh: 082237193"
            android:layout_marginLeft="16dp"
            android:background="@drawable/klik"
            android:fontFamily="@font/poppins_semibold"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/textView19"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_email_detailteam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cl_negara_detailteam">

        <TextView
            android:id="@+id/textView20"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:paddingTop="11dp"
            android:text="Email:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:hint="contoh: supriyadi@gmail.com"
            android:id="@+id/et_emailpemain_detailpemain"
            android:inputType="textEmailAddress"
            android:layout_width="189dp"
            android:layout_height="80dp"
            android:layout_marginLeft="16dp"
            android:layout_marginBottom="60dp"
            android:background="@drawable/klik"
            android:fontFamily="@font/poppins_semibold"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/textView20"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp">

    </LinearLayout>
</LinearLayout>